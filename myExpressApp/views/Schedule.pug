doctype html
head
  title Schedule
  link(href='https://fonts.googleapis.com/css?family=Roboto+Condensed' rel='stylesheet')
  link(rel='stylesheet' href='stylesheets/demo.css')
  link(rel='stylesheet' href='stylesheets/theme1.css')
  link(rel='stylesheet' href='stylesheets/profile.css')
  script(type='text/javascript' src='javascripts/links.js')
  script(type='text/javascript' src='javascripts/profile.js')
.topBar
  img.headerLogo(src='images/logoWhite.png' )
  form(action='/logout/', method='POST' enctype="multipart/form-data" class="logoutForm")
    button.logoutBtn(onclick="logout()") Log Out
.leftTab
  button.homeBtn Home
  br
  button.exploreBtn Explore
  br
  button.exploreBtn(onclick="profile()") Profile
.rightTab

  #caleandar
  script(type='text/javascript' src='javascripts/caleandar.js')
  script(type='text/javascript' src='javascripts/demo.js')
  div(class="events") 
    h2 Events 
    for x in dates
        p(class="Date"+x.Date, id="invisibleEvent", name="EventParagraph")=x.Name
        
  #none
    h2(class="date")
    img(src="images/add.svg" width="27",height="27", class="add", onclick='add()')
    form(action='/addToSchedule/',method='POST',class="eventForm")
      input#field(type='text', class="workoutEvent", placeholder="Workout Name", name="event") 
      input#InputDate(type="text",name="InputDate",class="invisible")
      input(type='submit',class="blue")
    button(class="red", onclick='cancel()') Cancel
