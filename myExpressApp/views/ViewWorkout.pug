doctype html
head
    meta(charset='UTF-8')
    meta(name='viewport' content='width=device-width, initial-scale=1.0')
    title Profile Page
    link(rel='stylesheet' href='stylesheets/post.css')
    script(type='text/javascript' src='javascripts/links.js')
    script(type='text/javascript' src='javascripts/workout.js')
.topBar
    img.headerLogo(src='images/logoWhite.png')
    button.postBtn + Post
    button.logoutBtn Log Out
.leftTab
    button.homeBtn Home
    br
    button.exploreBtn Explore
    br
    button.exploreBtn(onclick="profile()") Profile
.rightTab
    h1=x.Name
    h2=x.Caption 
    h3=x.Date
    b=focus.Name+': '
    b=focus.Weight+' lbs '
    b=focus.Sets +'x'
    b=focus.Reps 
    br
    video(src=focus.Pic width="600", height="400",  controls)
    br
    for ex in w 
      p(onclick="change()", id=ex.Name, class="pointer")=ex.Name
      form(action='/changeVideo/', method='POST',class='workoutForm')
        input#invisible(type='text',name="wName",class="wName")
        input#invisible(type='submit')
